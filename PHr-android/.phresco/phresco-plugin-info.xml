    <mojo>
        <goal>package</goal>
        <description>Goal which packages the Android Native project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>com.photon.phresco.plugins.android.AndroidPlugin</implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
			<parameters>
				<parameter>
                    <name>
                        <value lang="en">Build Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildName</key>
                    <multiple>false</multiple>
					<sort>false</sort>
                    <show>true</show>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Number</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
                    <multiple>false</multiple>
					<sort>false</sort>
                    <show>true</show>
                    <value></value>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Show Settings</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>showSettings</key>
                    <multiple>false</multiple>
					<sort>false</sort>
                    <show>true</show>
                    <value>false</value>
                    <dependency>environmentName</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.impl.EnvironmentsParameterImpl</class>
                        <dependencies>
                            <dependency>
								<groupId>com.photon.phresco.commons</groupId>
								<artifactId>phresco-commons</artifactId>
								<type>jar</type>
								<version>2.0.0.27002</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
                    <multiple>false</multiple>
					<sort>false</sort>
                    <show>true</show>
                    <value></value>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">SDK</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>sdk</key>
					<possibleValues>
                        <value key="2.2">2.2</value>
                        <value key="2.3.3">2.3.3</value>
                        <value key="4.0.3">4.0.3</value>
                    </possibleValues>
					<multiple>false</multiple>
					<sort>false</sort>
                    <show>true</show>
                    <value></value>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>true</key>
                            <value>-DskipTests=true</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>false</key>
                            <value>-DskipTests=false</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Skip Unit Test</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>skipTest</key>
                    <multiple>false</multiple>
                    <value>false</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>showErrors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>hideLogs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>showDebug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value key="showErrors">Show Errors</value>
                        <value key="hideLogs">Hide Logs</value>
                        <value key="showDebug">Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
					<sort>false</sort>
                    <show>true</show>
                    <value></value>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Proguard</value>
                    </name>
                    <type>boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>proguard</key>
                    <multiple>false</multiple>
					<sort>false</sort>
                    <show>true</show>
                    <value>false</value>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Signing</value>
                    </name>
                    <type>boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>signing</key>
                    <multiple>false</multiple>
					<sort>false</sort>
                    <show>true</show>
                    <value>false</value>
					<dependency>keystore,storepass,keypass,alias</dependency>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Keystore</value>
                    </name>
                    <type>FileBrowse</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>keystore</key>
                    <multiple>false</multiple>
					<sort>false</sort>
                    <show>false</show>
                    <value></value>
                    <fileType>keystore</fileType>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Storepass</value>
                    </name>
                    <type>String</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>storepass</key>
                    <multiple>false</multiple>
					<sort>false</sort>
                    <show>false</show>
                    <value></value>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Keypass</value>
                    </name>
                    <type>String</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>keypass</key>
                    <multiple>false</multiple>
					<sort>false</sort>
                    <show>false</show>
                    <value></value>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Alias</value>
                    </name>
                    <type>String</type>
                    <required>true</required>
                    <editable>true</editable>
                    <description></description>
                    <key>alias</key>
                    <multiple>false</multiple>
					<sort>false</sort>
                    <show>false</show>
                    <value></value>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
