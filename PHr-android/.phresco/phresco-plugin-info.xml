					<sort>false</sort>
                    <show>false</show>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
					<multiple>false</multiple>
					<sort>false</sort>
                    <show>false</show>
                    <value></value>
                </parameter>
            </parameters>
        </configuration>
